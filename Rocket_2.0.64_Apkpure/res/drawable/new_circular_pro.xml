<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:innerRadiusRatio="2.5" android:useLevel="false" android:thickness="8.0dip">
            <gradient android:startColor="#ffffffff" android:endColor="#ffdfdede" android:angle="0.0" android:type="radial" android:gradientRadius="100.0" />
            <stroke android:width="1.0dip" android:color="#ffdfdede" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:toDegrees="360.0">
            <shape android:shape="ring" android:innerRadiusRatio="2.5" android:thickness="8.0dip">
                <gradient android:startColor="@color/colorPrimaryDark" android:endColor="@color/colorPrimary" android:angle="0.0" android:type="radial" android:gradientRadius="100.0" />
            </shape>
        </rotate>
    </item>
</layer-list>