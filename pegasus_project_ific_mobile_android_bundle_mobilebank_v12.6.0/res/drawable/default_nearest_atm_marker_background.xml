<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="@dimen/default_nearest_atm_marker_icon_background_left_offset" android:top="@dimen/default_nearest_atm_marker_icon_background_top_offset">
        <rotate android:fromDegrees="-45.0" android:toDegrees="0.0" android:pivotX="0.0%" android:pivotY="0.0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/default_nearest_atm_marker_icon_background_color" />
                <size android:height="@dimen/default_nearest_atm_marker_icon_background_triangle_height" android:width="@dimen/default_nearest_atm_marker_icon_background_triangle_width" />
            </shape>
        </rotate>
    </item>
    <item android:bottom="@dimen/default_nearest_atm_marker_icon_background_bottom_offset">
        <shape android:shape="rectangle">
            <solid android:color="@color/default_nearest_atm_marker_icon_background_color" />
            <size android:height="@dimen/default_nearest_atm_marker_icon_background_rectangle_height" android:width="@dimen/default_nearest_atm_marker_icon_background_rectangle_width" />
            <corners android:radius="@dimen/default_nearest_atm_marker_icon_background_rectangle_corner_rounding" />
        </shape>
    </item>
</layer-list>