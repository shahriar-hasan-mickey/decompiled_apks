<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/saving_m30" />
            <corners android:radius="@dimen/default_account_background_radius" />
        </shape>
    </item>
    <item android:state_hovered="true">
        <shape>
            <solid android:color="@color/saving_m30" />
            <corners android:radius="@dimen/default_account_background_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/saving_m20" />
            <corners android:radius="@dimen/default_account_background_radius" />
        </shape>
    </item>
</selector>