<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <rotate android:fromDegrees="150.0" android:toDegrees="150.0">
            <shape android:shape="ring" android:innerRadiusRatio="3.0" android:thicknessRatio="30.0" android:useLevel="true">
                <gradient android:startColor="#ffd6d6d6" android:endColor="#ffd6d6d6" android:angle="0.0" android:type="sweep" android:centerColor="#ffd6d6d6" />
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="-270.0" android:toDegrees="-270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="3.0" android:thicknessRatio="30.0" android:useLevel="true">
                <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50.0%" android:pivotY="50.0%" />
                <gradient android:startColor="#ff65d573" android:endColor="#ff65d573" android:angle="0.0" android:type="sweep" android:centerColor="#ff65d573" />
            </shape>
        </rotate>
    </item>
</layer-list>