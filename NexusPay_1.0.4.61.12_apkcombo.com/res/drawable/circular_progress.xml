<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <shape android:shape="ring" android:innerRadiusRatio="2.2" android:thicknessRatio="22.0" android:useLevel="true">
            <gradient android:startColor="@color/kona_circular_progress_normal_bg" android:endColor="@color/kona_circular_progress_normal_bg" android:angle="0.0" android:type="linear" android:centerColor="@color/kona_circular_progress_normal_bg" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:innerRadiusRatio="2.2" android:thicknessRatio="22.0" android:useLevel="true">
            <gradient android:startColor="@color/fadedColorPrimary" android:endColor="@color/colorPrimaryRed" android:angle="90.0" android:centerColor="@color/colorPrimaryDark" />
        </shape>
    </item>
</layer-list>