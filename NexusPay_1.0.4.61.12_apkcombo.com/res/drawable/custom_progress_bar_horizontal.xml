<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffbfbfc1" android:endColor="#ffbfbfc1" android:angle="270.0" android:centerY="0.5" android:centerColor="#ffbfbfc1" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff4caf50" android:endColor="#ff4caf50" android:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>