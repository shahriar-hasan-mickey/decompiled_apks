<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/pressed_secure_keyboard_key_background" />
            <stroke android:width="@dimen/secure_keyboard_key_border_width" android:color="@color/secure_keyboard_key_border_color" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/normal_secure_keyboard_key_background" />
            <stroke android:width="@dimen/secure_keyboard_key_border_width" android:color="@color/secure_keyboard_key_border_color" />
        </shape>
    </item>
</selector>