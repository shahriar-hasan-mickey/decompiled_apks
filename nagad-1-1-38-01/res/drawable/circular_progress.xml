<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <shape android:shape="ring" android:innerRadiusRatio="2.3" android:thicknessRatio="18.0" android:useLevel="true">
            <stroke android:width="1.0dip" android:color="@color/kona_circular_progress_border_bg" />
            <gradient android:startColor="@color/kona_circular_progress_normal_bg" android:endColor="@color/kona_circular_progress_normal_bg" android:angle="0.0" android:type="linear" android:centerColor="@color/kona_circular_progress_normal_bg" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:innerRadiusRatio="2.3" android:thicknessRatio="18.0" android:useLevel="true">
            <gradient android:startColor="?progressCircleEndGradientColor" android:endColor="?colorPrimary" android:angle="0.0" android:type="linear" />
        </shape>
    </item>
</layer-list>